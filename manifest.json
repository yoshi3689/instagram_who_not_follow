{
	"manifest_version": 2,
	"name": "InstagramWhoFollows",
	"description": "CHeck who follows/not follow on instagram ",
	"version": "0.0.0",
  "permissions": [
    "activeTab","tabs", "storage"
  ],
	"icons": {
		"64": "icons/icon.png"
	},
	"background": {
		"scripts": [
			"background_script.js"
		]
	},
	"content_scripts": [
		{
			"matches": ["https://www.instagram.com/*"],
			"js": [
				"content_script.js"
			]
		}
	],
	"browser_action": {
		"default_icon": {
			"64": "icons/icon.png"
		},
		"default_popup": "browserAction/index.html",
		"default_title": "InstagramWhoFollows"
	},
    "web_accessible_resources": [
     "./urls.js"
  ]
}