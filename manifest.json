{
	"manifest_version": 2,
	"name": "InstagramWhoFollows",
	"description": "Check who follows/not follow on instagram ",
	"version": "0.0.0",
  "permissions": [
    "activeTab","tabs", "storage", "notifications"
  ],
	"background": {
		"scripts": [
			"background_script.js"
		]
	},
	"content_scripts": [
		{
			"matches": ["https://www.instagram.com/*"],
			"js": [
				"content_script.js"
			]
		}
	],
	"browser_action": {
  "default_icon": {
    "16": "icons/icon-128.svg",
    "32": "icons/icon-32.svg",
    "64": "icons/icon-128.svg",
    "128": "icons/icon-128.svg"
  },

		"default_popup": "browserAction/index.html",
		"default_title": "InstagramWhoFollows"
	},
    "web_accessible_resources": [
     "./urls.js"
  ]
}